    if(header && !seq) print "Missing sequence for: " header
    header=$0; seq=""
} 
!/^>/ && NF > 0 {seq=seq $0} 
END {
    if(header && !seq) print "Missing sequence for: " header
}' Saccharomyces.X3.final.fasta

